{
  "name": "Dotori 도토리",
  "version": "1.0",
  "description": "쉽게 사용하는 Git, Github",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "*://playentry.org/*",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://playentry.org/*"
      ],
      "all_frames": true,
      "js": [
        "injector.js",
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "real_content_script.js"
  ],
  "content_security_policy": "script-src 'self' 'sha256-eGQ/xaBz1+cKc18MM8k1rrbZJeGOxxmWxAr9W1vUMVs='; object-src 'self'"
}
